<!-- Template de registro --><script type="text/javascript">
  function getQueryVariable(variable) {
    var query = window.location.search.substring(1);
    var vars = query.split("&");
    for (var i=0;i<vars.length;i++) {
      var pair = vars[i].split("=");
      if(pair[0] == variable){return pair[1];}
    }
    return(false);
  }
  if(getQueryVariable('rmapp')){
    document.getElementsByTagName('header')[0].style.setProperty('display', 'none', 'important');
    document.querySelector('.m_breadcrumb').style.setProperty('display', 'none', 'important');
    document.querySelector('.aside').style.setProperty('display', 'none', 'important');
    document.querySelector('.pre_footer').style.setProperty('display', 'none', 'important');
    var navi = document.querySelector('.m_cap_legend_nav');
    if(navi) { navi.style.setProperty('display', 'none', 'important'); }
    var cont = document.querySelector('.aditional_content');
    if(cont) { cont.style.setProperty('display', 'none', 'important'); }
    var acti = document.querySelector('.actions_row');
    if(acti) { acti.style.setProperty('padding-top', '20px'); }
    var actc = document.querySelector('.actions_row .col')
    if(actc) { actc.style.setProperty('display', 'none', 'important'); }
    var pagi = document.querySelector('.m_pagination')
    if(pagi) { pagi.style.setProperty('display', 'none', 'important'); }
    var footer = document.getElementsByTagName('footer');
    footer[footer.length-1].style.setProperty('display', 'none', 'important');
  }
</script>
<script type="text/javascript/defer">
  if(getQueryVariable('rmapp')){
    var navc = document.querySelector('.m_link_nav_content');
    if(navc) { navc.style.setProperty('display', 'none', 'important'); }
  }
</script><script async defer src="/StaticFiles/RealMadridResponsive/javascripts/LAB.min.js" type="text/javascript" ></script>
<script type="text/javascript">

  function async(callback) {
    setTimeout(callback, 0);
  }

  function createDeferElement(theNode) {
    var newNode = document.createElement(theNode.tagName);
    if (newNode.tagName == 'SCRIPT') {
      //newNode.async = theNode.getAttribute('async') != null;
      if (theNode.getAttribute('defer-src') != null) {
        newNode.src = theNode.getAttribute('defer-src');
      } else {
        newNode.innerHTML = theNode.innerHTML;
      }
    } else {
      if (theNode.getAttribute('defer-href') != null) {
        newNode.href = theNode.getAttribute('defer-href');
        newNode.setAttribute('rel', 'stylesheet');
      }
    }
    newNode.type = theNode.getAttribute('type').replace('/defer', '');
    return newNode;
  }

  (function(w) {
    "use strict";
    var loadCSS = function(obj, before, media) {
      var doc = w.document;
      var ss = doc.createElement("link");
      var ref;
      if (before) {
        ref = before;
      } else {
        var refs = (doc.body || doc.getElementsByTagName("head")[0]).childNodes;
        ref = refs[refs.length - 1];
      }
      var sheets = doc.styleSheets;
      ss.rel = "stylesheet";
      ss.href = obj.getAttribute('defer-href');
      ss.media = "only x";

      function ready(cb) {
        if (doc.body) {
          return cb();
        }
        setTimeout(function() {
          ready(cb);
        });
      }
      ready(function() {
        ref.parentNode.insertBefore(ss, (before ? ref : ref.nextSibling));
      });
      var onloadcssdefined = function(cb) {
        var resolvedHref = ss.href;
        var i = sheets.length;
        while (i--) {
          if (sheets[i].href === resolvedHref) {
            return cb();
          }
        }
        setTimeout(function() {
          onloadcssdefined(cb);
        });
      };

      function loadCB() {
        if (ss.addEventListener) {
          ss.removeEventListener("load", loadCB);
        }
        ss.media = media || "all";
      }
      if (ss.addEventListener) {
        ss.addEventListener("load", loadCB);
      }
      ss.onloadcssdefined = onloadcssdefined;
      onloadcssdefined(loadCB);
      return ss;
    };
    if (typeof exports !== "undefined") {
      exports.loadCSS = loadCSS;
    } else {
      w.loadCSS = loadCSS;
    }
  }(typeof global !== "undefined" ? global : this));

  function dCSS() {
    var cssobjs = document.querySelectorAll('link[type="text/css/defer"]');
    for (var i = 0; i < cssobjs.length; i++) {
      loadCSS(cssobjs[i]);
      cssobjs[i].parentNode.removeChild(cssobjs[i]);
    }
  }

 function dTPJS() {
    var objs = document.querySelectorAll('script[type="text/javascript/defer"]'),
      inlineScript = '';
    for (var i = 0; i < objs.length; i++) {
      if (objs[i].getAttribute('defer-src') != null) {
        var newNode = document.createElement(objs[i].tagName);
        newNode.src = objs[i].getAttribute('defer-src');
        newNode.type = objs[i].getAttribute('type').replace('/defer', '');
        objs[i].parentNode.insertBefore(createDeferElement(objs[i]), objs[i]);
        objs[i].parentNode.removeChild(objs[i]);
      } else {
        inlineScript += objs[i].innerHTML;
      }
    }
    if (inlineScript.length > 0) {
      var newNode = document.createElement('script');
      newNode.innerHTML = inlineScript;
      objs[objs.length - 1].parentNode.insertBefore(newNode, objs[objs.length - 1]);
    }
    return async(dIFR);
  }

  function dIFR() {
    var vidDefer = document.getElementsByTagName('iframe');
    for (var i = 0; i < vidDefer.length; i++) {
      if (vidDefer[i].getAttribute('data-src')) {
        vidDefer[i].setAttribute('src', vidDefer[i].getAttribute('data-src'));
      }
    }
    return;
  }

  function dLJS() {
    $LAB
      .setOptions({
        AlwaysPreserveOrder: false,
        AllowDuplicates: false
      })
      .script('/StaticFiles/RealMadridResponsive/javascripts/cookie_realmadrid.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/jquery.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/all.fn_home.js')
	  
	  
      .script('/StaticFiles/RealMadridResponsive/javascripts/i18n.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/popups.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/search_results.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/calendar.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/gallery_modificado.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/cabecera_login_usuario.js')
	  //.script('/StaticFiles/RealMadridResponsive/javascripts/video_player.js')
      .script('/StaticFiles/RealMadridResponsive/javascripts/all.js').wait(function() {
        RM._execute_callbacks('load');
      });
    return async(dTPJS);
  }

  function defer() {
    return window.isLoaded = true, dCSS(), dLJS();
  }
  if (window.addEventListener)
    window.addEventListener("load", defer, false);
  else if (window.attachEvent)
    window.attachEvent("onload", defer);
  else window.onload = defer;

</script></body>
</html>

