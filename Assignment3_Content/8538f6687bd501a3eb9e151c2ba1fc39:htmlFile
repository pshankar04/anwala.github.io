<script>
  var ref = document.referrer || '';
  if (ref !== 'https://ameblo.jp/') {
    var RANGE = 100;
    var roundedSpeedIndex = Math.round(RUMSpeedIndex());
    
    try {
      var observer = new PerformanceObserver(function (list) {
        var firstPaint = NaN;
        var firstContentfulPaint = NaN;
        var paint = list.getEntriesByType('paint') || [];
        paint.forEach(function(entry) {
          if (entry.name === 'first-paint') {
            firstPaint = entry.startTime + entry.duration;
          } else if (entry.name === 'first-contentful-paint') {
            firstContentfulPaint = entry.startTime + entry.duration;
          }
        });

        // 以下のルールで値を丸める
        // 100: 1~100, 1000: 901~1000, 1100: 1001~1100
        var roundedFirstPaint = Math.round(firstPaint);
        var roundedFirstContentfulPaint = Math.round(firstContentfulPaint);
        dataLayer.push({
          event: 'ga_pageview',
          firstContentfulPaint: roundedFirstContentfulPaint,
          firstContentfulPaintRounded: Math.ceil(roundedFirstContentfulPaint / RANGE) * RANGE,
          firstPaint: roundedFirstPaint,
          firstPaintRounded: Math.ceil(roundedFirstPaint / RANGE) * RANGE,
          speedIndex: roundedSpeedIndex,
          speedIndexRounded: Math.ceil(roundedSpeedIndex / RANGE) * RANGE,
        });

        observer.disconnect();
      });
      observer.observe({entryTypes: ['paint']});
    } catch (err) {
      dataLayer.push({
        event: 'ga_pageview',
        firstContentfulPaint: NaN,
        firstContentfulPaintRounded: NaN,
        firstPaint: NaN,
        firstPaintRounded: NaN,
        speedIndex: roundedSpeedIndex,
        speedIndexRounded: Math.ceil(roundedSpeedIndex / RANGE) * RANGE,
      });
    }
  } else {
    // 疑似SPAページではイベントのみpushする
    dataLayer.push({
      event: 'ga_pageview',
    });
  }
</script>

