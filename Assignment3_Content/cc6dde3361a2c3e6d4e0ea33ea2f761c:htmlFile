e,&quot;verified_at&quot;:null,&quot;summary&quot;:&quot;State House of Representatives - California-070&quot;,&quot;locale&quot;:&quot;en-US&quot;,&quot;confirmed_at&quot;:&quot;2015-02-06T21:42:46Z&quot;,&quot;is_person&quot;:true,&quot;member_of&quot;:{},&quot;additional_data&quot;:{&quot;state&quot;:&quot;CA&quot;,&quot;title&quot;:&quot;State Representative&quot;,&quot;district&quot;:&quot;070&quot;,&quot;active&quot;:true}},{&quot;id&quot;:761846,&quot;display_name&quot;:&quot;Jim Patterson&quot;,&quot;email&quot;:null,&quot;type&quot;:&quot;Politician&quot;,&quot;slug&quot;:&quot;james-patterson&quot;,&quot;description&quot;:null,&quot;photo_id&quot;:null,&quot;publicly_visible&quot;:false,&quot;verified_at&quot;:null,&quot;summary&quot;:&quot;State House of Representatives - California-023&quot;,&quot;locale&quot;:&quot;en-US&quot;,&quot;confirmed_at&quot;:&quot;2014-01-17T23:15:34Z&quot;,&quot;is_person&quot;:true,&quot;member_of&quot;:{},&quot;additional_data&quot;:{&quot;state&quot;:&quot;CA&quot;,&quot;title&quot;:&quot;State Representative&quot;,&quot;district&quot;:&quot;023&quot;,&quot;active&quot;:true}},{&quot;id&quot;:7750598,&quot;display_name&quot;:&quot;Sharon Quirk-Silva&quot;,&quot;email&quot;:null,&quot;type&quot;:&quot;Politician&quot;,&quot;slug&quot;:&quot;sharon-quirk-silva-3&quot;,&quot;description&quot;:null,&quot;photo_id&quot;:null,&quot;publicly_visible&quot;:false,&quot;verified_at&quot;:null,&quot;summary&quot;:&quot;State House of Representatives - California-065&quot;,&quot;locale&quot;:null,&quot;confirmed_at&quot;:&quot;2017-01-01T07:12:46Z&quot;,&quot;is_person&quot;:true,&quot;member_of&quot;:{},&quot;additional_data&quot;:{&quot;state&quot;:&quot;CA&quot;,&quot;title&quot;:&quot;State Representative&quot;,&quot;district&quot;:&quot;065&quot;,&quot;active&quot;:true}},{&quot;id&quot;:7750874,&quot;display_name&quot;:&quot;Eloise Reyes&quot;,&quot;email&quot;:null,&quot;type&quot;:&quot;Politician&quot;,&quot;slug&quot;:&quot;