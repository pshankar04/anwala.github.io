        var slidertimer, footerslidertimer;
        setTimeout(function(){
            $(".slider").css('right', '0px');
            $(".slider .close").html('<i class="fa fa-chevron-right"></i>');
            
            $(".footer-slider").css('bottom', '0px');
            $(".footer-slider .footer-close").html('<i class="fa fa-chevron-down"></i>');
            slidertimer = setTimeout(function(){
                $(".slider").css('right', '-321px');
                $(".slider .close").html('<i class="fa fa-chevron-left"></i>');
                
                $(".footer-slider").css('bottom', '-112px');
                $(".footer-slider .footer-close").html('<i class="fa fa-chevron-up"></i>');
            },10000);
        },2000);
        
        $(".close").on("click", function() {
            clearTimeout(slidertimer);
            var val = $(".slider").css('right');
            if(val == "0px") {
                $(".slider").css('right', '-321px');
                $(".slider .close").html('<i class="fa fa-chevron-left"></i>');
            } else {
                $(".slider").css('right', '0px');
                $(".slider .close").html('<i class="fa fa-chevron-right"></i>');
            }
        });
        $(".footer-close").on("click", function() {
            clearTimeout(slidertimer);
            var val = $(".footer-slider").css('bottom');
            if(val == "0px") {
                $(".footer-slider").css('bottom', '-112px');
                $(".footer-slider .footer-close").html('<i class="fa fa-chevron-up"></i>');
            } else {
                $(".footer-slider").css('bottom', '0px');
                $(".footer-slider .footer-close").html('<i class="fa fa-chevron-down"></i>');
            }
        });

