<script type='text/javascript/defer'>
		var ag_cid = '4500014100834';
	</script><script type='text/javascript/defer'>
		var reg_c = 'Page';
	</script><script type='text/javascript/defer'>

	function getCookie(name){
	if (document.cookie.length>0)
	  {
	  c_start=document.cookie.indexOf(name + "=");
	  if (c_start!=-1)
	    {
	    c_start=c_start + name.length+1;
	    c_end=document.cookie.indexOf(";",c_start);
	    if (c_end==-1) c_end=document.cookie.length;
	    return unescape(document.cookie.substring(c_start,c_end));
	    }
	  }
	return "";
	} 
	
	var pageSection = "";
	var pageType = "";
	
	cookieUsuario = getCookie("usuario").split('@');
	
	if (cookieUsuario == ""){
		cookieUsuario[0] = "no logado";
		cookieUsuario[5] = "";
		cookieUsuario[6] = "";
		cookieUsuario[7] = "";
		cookieUsuario[8] = "";
		pageSection = 'Home';
		pageType = 'Home';
	}else{
	  	if (cookieUsuario[0] == "socio"){
	  		pageSection = 'Oficina de Atencion al Socio';
			pageType = 'Area Privada de Usuario';
		}else if (cookieUsuario[0] == "carnetmadridista"){
			pageSection = 'Area Privada Madridistas';
			pageType = 'Area Privada de Usuario';
		}else if (cookieUsuario[0] == "usuarioregistrado"){
			pageSection = 'Home';
			pageType = 'Home';
		}
	}
	
    dataLayer = [{
	  'pageSeccion': pageSection,
	  'pageNivel': '',
	  'pageTypeNom': pageType,
	  'pageHier': 'Home | ',
	  'ventajaNom': '',
      'pageLang': 'es',
	  'pageMoment': '',
	  'partidoTitle': '',
	  'partidoDate': '',
	  'partidoTime': '',
	  'partidoTorneo': '',
	  'partidoEquipo': '',
	  'partidoContrincante': '',
	  'partidoBestPlayer': '',
	  'partidoResultado': '',
	  'personaNom': '',
	  'noticiaCat': '',
	  'noticiaType': '',
	  'userType':cookieUsuario[0].replace(/\"/g,""),
	  'userEdadCodi':cookieUsuario[5].replace(/\"/g,""),
	  'userGenNom':cookieUsuario[6].replace(/\"/g,""),
	  'userID':cookieUsuario[7].replace(/\"/g,""),
	  'userLoyalty':cookieUsuario[8].replace(/\"/g,""),
	  'gtm.blacklist': ['<html>']
    }];
	
</script>
